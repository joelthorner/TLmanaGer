{
  "name": "tlmanager",
  "description": "Development tool",
  "version": null,
  "manifest_version": 2,
  "icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
  "browser_action": {
    "default_title": "tlmanager",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
	"options_page": "options/options.html",
	"permissions": [
		"storage"
	]
}
